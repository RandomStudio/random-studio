{
  "name": "random.studio",
  "private": true,
  "description": "Gatsby based site for Random Studio",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop --host 0.0.0.0 --https",
    "develop:vanilla": "gatsby clean && gatsby develop --host 0.0.0.0",
    "develop:expose": "gatsby clean && npm run develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "lint": "eslint './src/**/*.js'"
  },
  "dependencies": {
    "babylonjs": "4.1.0",
    "babylonjs-loaders": "4.1.0",
    "framer-motion": "^2.0.0-beta.33",
    "gatsby": "^2.25.4",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-google-analytics": "^2.2.1",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-netlify-cms": "RandomStudio/gatsby#plugin-only",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "2.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "3.2.1",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.2",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.12.3",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-sass": "^4.13.1",
    "postcss-custom-properties": "^9.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "react-markdown": "^4.3.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.9.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
